{
  "name": "beryldb/php-beryl",
  "description": "PHP client for BerylDB.",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "beryldb",
    "key-value",
    "php client"
  ],
  "support": 
  {
      "issues": "https://github.com/beryldb/php-beryl/issues"
  },
  "require": {
    "php": "^7.3|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^9.0"
  },
  "autoload": {
    "files":  ["src/Connection/Protocol.php"],
    "psr-4": {
     "Beryl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Beryl\\Tests\\": "tests/"
    }
  },
  "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
  },  
  "authors": [
    {
      "name": "Carlos Ferry",
      "email": "cferry@beryldb.com"
    }
  ]
}
